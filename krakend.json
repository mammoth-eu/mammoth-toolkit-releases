{
  "version": 3,
  "timeout": "60s",
  "extra_config": {
    "router": {
      "auto_options": true,
      "return_error_msg": true
    },
    "security/cors": {
      "allow_origins": [
        "http*"
      ],
      "allow_methods": [
        "GET", "HEAD", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"
      ],
      "expose_headers": [
        "Content-Length",
        "Content-Type"
      ],
      "allow_headers": [
        "Origin",
        "Authorization",
        "Content-Type",
        "Accept-Language"
      ],
      "max_age": "12h",
      "debug": true
    }
  },
  "endpoints":[
  {
    "endpoint": "/keycloak-protected",
    "method": "GET",
    "input_headers": [
      "Authorization",
      "x-user-id",
      "x-user-name",
      "x-user-email"
    ],
    "extra_config": {
        "auth/validator": {
            "alg": "RS256",
            "jwk_url": "http://keycloak.local.exus.ai:8080/realms/toolkit/protocol/openid-connect/certs",
            "disable_jwk_security": true,
            "propagate_claims": [
              ["sub", "x-user-id"],
              ["name", "x-user-name"],
              ["email", "x-user-email"]
          ],
			"cache": true
        }
    },
    "backend": [
        {
          "host":["http://api:8000"],
          "url_pattern": "/keycloak-protected",
          "extra_config": {
            "backend/http": {
              "return_error_code": true
            }
          }
        }
    ]
  },
